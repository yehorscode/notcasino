// This file tells Vercel to use npm for installing dependencies and building the project
{
  "version": 2,
  "builds": [
    { "src": "vite.config.ts", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
